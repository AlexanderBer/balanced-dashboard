<div class="results">
  <div class="items-wrapper">
    <table class="items table">
      <thead>
        <tr>
          <th></th>
          {{#each view.table_headers}}
            <th class="label6a">{{this}}</th>
          {{/each}}
        </tr>
      </thead>
      <tbody>
      {{#each view.table_rows}}
        <tr>
          <td>
            {{#if isValid}}
              {{#if credit.isNew}}
                {{#if credit.isSaving}}<i class="icon icon-loading"></i>{{/if}}
              {{else}}
                <i class="icon icon-checkmark"></i>
              {{/if}}
            {{else}}
              <i class="icon icon-alert"></i>
            {{/if}}
          </td>
          {{#each columns}}
            <td><a href="#"><span>{{this}}</span></a></td>
          {{/each}}
        </tr>
      {{/each}}
      </tbody>
    </table>
  </div>
</div>
